{
  "name": "visu",
  "author": {
    "name": "j-o-d-o",
    "email": "jdobler@protonmail.com"
  },
  "version": "1.0.0",
  "description": "visu for driving",
  "main": "app/src/main.js",
  "homepage": "http://hompage.com",
  "license": "ISC",
  "scripts": {
    "build-sass": "node-sass --include-path node_modules --include-path src src/index.scss app/src/index.css --output-style compressed",
    "start": "build-sass && tsc && electron ./app/src/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "visu.id",
    "extraFiles": {
      "from": "./assets",
      "to": "./dist"
    },
    "linux": {
      "icon": "assets/logo.png",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "babylonjs": "^4.0.3",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-redux": "^7.1.1",
    "redux": "^4.0.4",
    "rmwc": "^5.7.1",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-redux": "^7.1.5",
    "@types/styled-components": "4.1.8",
    "electron": "^6.0.12",
    "electron-builder": "^21.2.0",
    "node-sass": "^4.12.0",
    "typescript": "^3.6.4"
  }
}
