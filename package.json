{
  "name": "visu",
  "author": {
    "name": "j-o-d-o",
    "email": "jdobler@protonmail.com"
  },
  "version": "1.0.0",
  "description": "visu for driving",
  "main": "app/src/main.js",
  "homepage": "http://hompage.com",
  "license": "ISC",
  "scripts": {
    "build-sass": "node-sass --include-path node_modules --include-path src src/index.scss app/src/index.css --output-style compressed",
    "start": "npm run build-sass && tsc && electron ./app/src/main.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "rm -rf node_modules/@types/react-native && electron-builder install-app-deps"
  },
  "build": {
    "appId": "visu.id",
    "extraFiles": {
      "from": "./assets",
      "to": "./dist"
    },
    "linux": {
      "icon": "assets/logo.png",
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "dependencies": {
    "@types/classnames": "^2.2.9",
    "babylonjs": "^4.0.3",
    "babylonjs-gui": "^4.0.3",
    "babylonjs-materials": "^4.0.3",
    "jpeg-js": "^0.3.6",
    "node-ipc": "^9.1.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-redux": "^7.1.3",
    "redux": "^4.0.5",
    "rmwc": "^5.7.2",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/node-ipc": "^9.1.2",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/styled-components": "4.1.8",
    "@types/node": "12.12.6",
    "electron": "^7.1.7",
    "electron-builder": "^22.1.0",
    "electron-devtools-installer": "^2.2.4",
    "node-sass": "^4.13.0",
    "typescript": "3.6.4"
  }
}
